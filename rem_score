    STRUCT('Kevin Alejo', 'JFG', 'Tier 2', 'No'),
    STRUCT('Mark Sibal', 'JFG', 'Tier 2', 'No'),
    STRUCT('Mary Esguerra', 'JFG', 'Tier 2', 'No'),
    STRUCT('Ramil Dimacuha', 'JFG', 'Tier 1', 'No'),
