WITH reviewer_metadata AS (
  SELECT * FROM UNNEST([
    -- JFG Squad (from latest T1/T2 list)
    STRUCT('Adriane Guzman' AS reviewed_by, 'JFG' AS squad_code, 'Tier 2' AS review_tier, 'No' AS tier_2_audit),
