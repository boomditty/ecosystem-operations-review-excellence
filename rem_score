    STRUCT('Bernard Alfonso', 'NOS', 'Tier 2', 'No'),
    STRUCT('Blue Bautista', 'NOS', 'Tier 2', 'No'),
    STRUCT('Camille Quizon', 'NOS', 'Tier 2', 'No'),
    STRUCT('Caryl Rivera', 'NOS', 'Tier 2', 'No'),
